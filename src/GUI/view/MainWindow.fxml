                                 </children>
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor></AnchorPane>
                        </content>
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                           <effect>
                              <Blend />
                           </effect>
                      </TitledPane>
                      <TitledPane animated="false" text="Load File">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <Button fx:id="btnLoadGPS" layoutX="31.0" layoutY="18.0" mnemonicParsing="false" onAction="#loadGPSFile" prefHeight="27.0" prefWidth="140.0" text="Load GPS File" />
                                    <Label fx:id="lbl_loadGPSInfo" layoutX="32.0" layoutY="52.0" prefHeight="49.0" prefWidth="140.0" text="Load your GPS File" textAlignment="CENTER" wrapText="true" />
                                    <Label layoutX="24.0" layoutY="120.0" prefHeight="42.0" prefWidth="161.0" text="note: has to be a .gpx File until now." textAlignment="CENTER" wrapText="true" />
                                 </children></AnchorPane>
                        </content>
                           <effect>
                              <Blend />
                           </effect>
                      </TitledPane>
                      <TitledPane animated="false" text="Craft">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <Button fx:id="btn_drawCourse" layoutX="28.0" layoutY="73.0" mnemonicParsing="false" onAction="#drawCourse" prefHeight="27.0" prefWidth="139.0" text="Draw Course" />
                                    <Button fx:id="btn_showRoute" layoutX="68.0" layoutY="139.0" mnemonicParsing="false" onAction="#drawRoute" text="Show Route" />
                                 </children></AnchorPane>
                        </content>
                      </TitledPane>
                    </panes>
                  </Accordion>
               </children>
            </AnchorPane>
         </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
